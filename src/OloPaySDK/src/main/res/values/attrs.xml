<resources>
    <attr name="postalCodeEnabled" format="boolean" />
    <attr name="displayErrors" format="boolean" />
    <attr name="cardNumberHint" format="string" />
    <attr name="expirationHint" format="string" />
    <attr name="cvcHint" format="string" />
    <attr name="postalCodeHint" format="string" />

    <declare-styleable name="PaymentCardDetailsSingleLineView">
        <attr name="postalCodeEnabled" />
        <attr name="displayErrors" />
        <attr name="cardNumberHint" />
        <attr name="expirationHint" />
        <attr name="cvcHint" />
        <attr name="postalCodeHint" />
    </declare-styleable>

    <declare-styleable name="PaymentCardDetailsMultiLineView">
        <attr name="postalCodeEnabled" />
        <attr name="displayErrors" />
        <attr name="cardNumberHint" />
        <attr name="expirationHint" />
        <attr name="cvcHint" />
        <attr name="postalCodeHint" />
    </declare-styleable>

    <declare-styleable name="PaymentCardDetailsForm">
        <attr name="cardNumberHint" />
        <attr name="expirationHint" />
        <attr name="cvcHint" />
        <attr name="postalCodeHint" />
    </declare-styleable>
</resources>